        </misc:Lokalisirung>
      </misc:Name>
      <misc:Beschreibung>
        <misc:Lokalisirung Sprache="de">
          <![CDATA[]]>
        </misc:Lokalisirung>
      </misc:Beschreibung>
      <misc:Levels>
        <misc:Level Id="T"></misc:Level>
      </misc:Levels>
    </Pfad>
  </Pfade>