<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        xmlns:nota="https://nota-game2.azurewebsites.net/schema/vNext/nota"
        xmlns:misc="https://nota-game2.azurewebsites.net/schema/vNext/misc"
        xmlns:wesen="https://nota-game2.azurewebsites.net/schema/vNext/lebewesen"
        xmlns:pfad="https://nota-game2.azurewebsites.net/schema/vNext/pfad"
        xmlns:profession="https://nota-game2.azurewebsites.net/schema/vNext/profession"
        xmlns:talent="https://nota-game2.azurewebsites.net/schema/vNext/talent"
        xmlns:fertigkeit="https://nota-game2.azurewebsites.net/schema/vNext/fertigkeit"
        xmlns:besonderheit="https://nota-game2.azurewebsites.net/schema/vNext/besonderheit"
        targetNamespace="https://nota-game2.azurewebsites.net/schema/vNext/fertigkeit"
        elementFormDefault="qualified">
  <import namespace="https://nota-game2.azurewebsites.net/schema/vNext/misc" schemaLocation="misc.xsd"></import>
  <import namespace="https://nota-game2.azurewebsites.net/schema/vNext/lebewesen" schemaLocation="lebewesen.xsd"></import>
  <import namespace="https://nota-game2.azurewebsites.net/schema/vNext/pfad" schemaLocation="pfad.xsd"></import>
  <import namespace="https://nota-game2.azurewebsites.net/schema/vNext/talent" schemaLocation="talent.xsd"></import>
  <import namespace="https://nota-game2.azurewebsites.net/schema/vNext/besonderheit" schemaLocation="besonderheit.xsd"></import>
  <element name="Fertigkeiten">
    <complexType>
      <sequence>
        <element name="Fertigkeit" minOccurs="0" maxOccurs="unbounded">
          <complexType>
            <sequence>
              <element name="Name" type="misc:Lokalisierungen">
                <unique name="SpracheUnique">
                  <selector xpath="misc:Lokalisirung"/>
                  <field xpath="./@Sparche"/>
                  <field xpath="./@Geschlecht"/>
                </unique>
              </element>
              <element name="Beschreibung" type="misc:Lokalisierungen">
                <unique name="SpracheUnique1">
                  <selector xpath="misc:Lokalisirung"/>
                  <field xpath="./@Sparche"/>
                  <field xpath="./@Geschlecht"/>
                </unique>
              </element>
              <element name="Verbreitung" type="misc:Verbreitung"/>
              <element name="Tags" maxOccurs="1" minOccurs="0" >
                <complexType>
                  <sequence>
                    <element ref="misc:Tag" minOccurs="1" maxOccurs="unbounded"/>
                  </sequence>
                </complexType>
              </element>
              <element name="Ersetzt" minOccurs="0">
                <complexType>
                  <sequence>
                    <element ref="fertigkeit:Fertigkeit" minOccurs="1" maxOccurs="1"/>
                  </sequence>
                </complexType>
              </element>
              <element name="Voraussetzung" type="misc:BedingungsAuswahl" minOccurs="0"/>

            </sequence>
            <attribute type="string" name="Id" use="required"/>
            <attribute type="int" name="Kosten" use="required"/>
          </complexType>
        </element>
      </sequence>
    </complexType>
  </element>

  <element name="Fertigkeit" type="misc:NamedType"/>


</schema>